    <item>
      <title><![CDATA[newsletter_popup]]></title>
      <identifier><![CDATA[newsletter_popup]]></identifier>
      <content><![CDATA[<div class="block-content">
	<div class="form-subscribe-header block-title">
        <label for="newsletter-popup">signup for our newsletter & promotions</label>
    </div>
	<h2 class="promotions promotions1">Get</h2>
    <h2 class="promotions promotions2">50%<span class="off">Off</span></h2>
    <h3>on your next purchase</h3>
	<div class="subcribe-box newsletter-new clearfix">
		<div class="field newsletter input-box">
			<div class="control">
				<input name="email" type="email" id="newsletter-popup" class="input-text"
							placeholder="Enter your email address"
							data-validate="{required:true, 'validate-email':true}"/>
			</div>
		</div>
		<div class="actions">
			<button class="action subscribe button" title="Subscribe" type="submit">
				<span>Subscribe</span>
			</button>
		</div>
	</div>
    <div class="subscribe-bottom">
        <input type="checkbox" />Don’t show this popup again
    </div>
</div>]]></content>
